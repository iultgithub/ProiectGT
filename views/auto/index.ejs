<!doctype html>
<html>
    <head>
        <title>Auto</title>
        <link
        href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css"
        rel="stylesheet"
        integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN"
        crossorigin="anonymous"
    />
    </head>
    <body>
        <div class="container w-50">
            <h1 class="mb-4">Auto Inventar</h1>
            <a href="/autoroute/add" class="btn btn-success">Add Auto</a>
            <% auto.forEach(auto => { %>
            <div class="card mt-4">
                <div class="card-body">
                    <h4 class="card-title"><%= auto.nrauto %></h4>
                    <a
                        href="autoroute/<%= auto.id %>"
                        class="btn btn-primary"
                        >View More</a
                    >

                    <a href="autoroute/<%= auto.id %>/edit" class="btn btn-info">Edit</a>
                </div>
            </div>
            <% }) %>
        </div>
    </body>
</html>
